{# CodeSiren #}
<div id="divCodeSiren" class="row">
    <label class="col-2">Code Siren :</label>
    <div class="col">
        {{ sirene.codeSiren }}
    </div>
</div>

{# Dénomination #}
<div id="divDenomination" class="row">
    <label class="col-2">Dénomination :</label>
    <div class="col">
        {{ sirene.uniteLegale.denomination }}
    </div>
</div>

{# $adresseSiege #}
<div id="divStatut" class="row">
    <label class="col-2">Adresse du siège :</label>
    <div class="col">
        {{ sirene.adresseSiege.complementAdresse }}
        <br/>{{ sirene.adresseSiege.numeroVoie }}{{ sirene.adresseSiege.indiceRepetition }} {{ sirene.adresseSiege.typeVoie }} {{ sirene.adresseSiege.libelleVoie }}
        <br/>{{ sirene.adresseSiege.codePostal }} {{ sirene.adresseSiege.libelleCommune }} {{ sirene.adresseSiege.codeCedex }} {{ sirene.adresseSiege.libelleCedex }}
        {% if sirene.adresseSiege.codePaysEtranger != null %}
            <br/>{{ sirene.adresseSiege.libellePaysEtranger }}
        {% endif %}
    </div>
</div>

{# $statut #}
<div id="divStatut" class="row">
    <label class="col-2">Etat administratif :</label>
    <div class="col">
        {{ sirene.uniteLegale.statut }}
    </div>
</div>
    
{# $categorieJuridique #}
<div id="divCategorieJuridique" class="row">
    <label class="col-2">Catégorie juridique :</label>
    <div class="col">
