    </div>
</div>

{# $economieSocialeSolidaire #}
<div id="divEconomieSocialaSolidaire" class="row">
    <label class="col-2">Economie sociale et solidaire :</label>
    <div class="col">
        {{ sirene.uniteLegale.economieSocialeSolidaire }}
    </div>
</div>

{# $caractereEmployeur #}
<div id="divCarcactereEmployeur" class="row">
    <label class="col-2">Entreprise employeuse :</label>
    <div class="col">
        {{ sirene.uniteLegale.caractereEmployeur }}
    </div>
</div>    

{# $trancheEffectif #}
{# $anneeTrancheEffectif #}
<div id="divTrancheEffectif" class="row">
    <label class="col-2">Tranche d'effectifs :</label>
    <div class="col">
        {{ sirene.uniteLegale.trancheEffectif | trancheEffectifs}}
        {% if sirene.uniteLegale.trancheEffectif != "NN" and sirene.uniteLegale.trancheEffectif %}
            (en {{ sirene.uniteLegale.anneeTrancheEffectif }})
        {% endif %}
    </div>
</div>    

{# $nicSiege #}
<div id="divNicSiege" class="row">
    <label class="col-2">Code NIC du siège :</label>
    <div class="col">
        {{ sirene.uniteLegale.nicSiege }}
    </div>
</div>    

{# $categorieEntreprise #}
{# $anneeCategorieEntreprise #}
<div id="divCategorieEntreprise" class="row">
    <label class="col-2">Catégorie d'entreprise :</label>
    <div class="col">
        {{ sirene.uniteLegale.categorieEntreprise }} (en {{ sirene.uniteLegale.anneeCategorieEntreprise }})
    </div>
</div>    

{# $nbEtablissementsActifs #}
<div id="divCategorieEntreprise" class="row">
    <label class="col-2">Nb établissements actifs :</label>
    <div class="col">
        {{ sirene.nbEtablissementsActifs }}
    </div>
</div>    

{# $nbEtablissementsFermés #}
<div id="divCategorieEntreprise" class="row">
    <label class="col-2">Nb établissements fermés :</label>
    <div class="col">
        {{ sirene.nbEtablissementsFermés }}
    </div>
</div>

{# $Etablissements #}
<div id="divEtablissements" class="row">
    <label class="col-2">Etablissements actifs :</label>
    <div class="col">
        {% for etablissement in sirene.etablissements %}
            {% if loop.first %}
            <table border='1'>
                <tr>
                    <th>Etab</th>
                    <th>nic</th>
                    <th>Adresse</th>
                    <th>Effectifs</th>
                    <th>Activité principale</th>
                    <th>Enseigne</th>
                    <th>Dénomination</th>
                    <th>Employeur</th>
                </tr>
            {% endif %}
            <tr>
                <td>
                    {# etablissementSiege #}
                    {% if etablissement.etablissementSiege %}
                        siège
                    {% else %}
                        Etab.
                    {% endif %}
                </td>
                <td>
                    {# nic #}
                    {{ etablissement.nic }}
                </td>
                <td>
                    {# adresse #}
                    {{ etablissement.adresse.complementAdresse }}
                    <br/>{{ etablissement.adresse.numeroVoie }}{{ etablissement.adresse.indiceRepetition }} {{ etablissement.adresse.typeVoie }} {{ etablissement.adresse.libelleVoie }}
                    <br/>{{ etablissement.adresse.codePostal }} {{ etablissement.adresse.libelleCommune }} {{ etablissement.adresse.codeCedex }} {{ etablissement.adresse.libelleCedex }}
                    {% if etablissement.adresse.codePaysEtranger != null %}
                        <br/>{{ etablissement.adresse.libellePaysEtranger }}
                    {% endif %}
                </td>
                <td>
                    {# trancheEffectifs #}
                    {# anneEffectifs #}
                    {{ etablissement.trancheEffectifs | trancheEffectifs }}
                    {% if etablissement.trancheEffectifs != "NN" and etablissement.trancheEffectifs %}
                        <br/>en {{ etablissement.anneEffectifs }}
                    {% endif %}
                </td>
                <td>
                    {# activitePrincipale #}
                    {# nomenclatureActivitePrincipale #}
                    {{ etablissement.activitePrincipale }}
                    <br/>selon nomenclature {{ etablissement.nomenclatureActivitePrincipale }}
                </td>
                <td>
                    {# enseigne1 #}
                    {# enseigne2 #}
                    {# enseigne3 #}
                    {{ etablissement.enseigne1 }}
                    <br/>{{ etablissement.enseigne2 }}
                    <br/>{{ etablissement.enseigne3 }}
                </td>
                <td>
                    {# denominationUsuelle #}
                    {{ etablissement.denominationUsuelle }}
                </td>
                <td>
                    {# caractereEmployeur #}
                    {{ etablissement.caractereEmployeur }}
                </td>
            </tr>
            {% if loop.last %}
                </table>
            {% endif %}
        {% endfor %}
    </div>
</div>
